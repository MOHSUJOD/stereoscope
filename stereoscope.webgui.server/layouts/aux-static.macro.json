	"initValue": "-Inf dB",
	"coords": {"x": ${x},"y": ${y + 300}},
	"width": 40,
	"height": 20,
	"color": "green",
	"visibility": true,
	"orient": "horizontal",
	"background": true,
	"bgColor": "null",
	"edge": false,
	"size": 12
},
{
	"type": "staticLabel",
	"name": "label auxSend${n}",
	"oscMessage": "/null",
