{
	"type": "stdToggleButton",
	"name": "Mute",
	"oscMessage": "/stereoscope/input/${n}/toAux/${m}/channelOn",
	"dataType": "boolean",
	"initValue": false,
	"coords": {"x": ${x}, "y": ${y}},
	"width": 40,
	"height": 40,
	"color": "green",
	"visibility": true,
	"orient": "horizontal",
	"label": true
},
{
	"type": "volumeFader",
	"name": "Volume AuxSend%n",
	"oscMessage": "/stereoscope/input/%n/toAux/%m/level",
	"dataType": "float",
	"initValue": 0.0,
	"coords": {"x": ${x}, "y": ${y + 50}},
	"width": 40,
	"height": 250,
	"color": "green",
	"visibility": true,
	"orient": "horizontal",
	"mode": "normal"
},
{
	"type": "valueLabel",
	"name": "Loudness AuxSend${n}",
	"oscMessage": "/stereoscope/input/${n}/toAux/${m}/levelLabel",
