/**
 * This is the macro for the parametric equalizer
 * in channel strip view.
 */
{
	"type": "frame",
	"name": "PEQ Outer Frame",
	"oscMessage": "/null",
	"dataType": "string",
	"initValue": "Parametric Equalizer",
